  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "wsgi:app",
        "APP_ENV": "development",
        "FLASK_DEBUG": "1"
      },
      "args": ["run", "--port", "5000"],
      "jinja": true,
      "justMyCode": false
    },
    {
      "name": "bot",
      "type": "python",
      "request": "launch",
      "module": "celery",
      "args": [
        "-A",
        "app.controllers.celery:celery",
        "worker",
        "-B",
        "--concurrency=1"
      ]
    },
    {
      "name": "bot cmd",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "args": ["bot"]
    },
    {
      "name": "db-tickets-populate",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "args": ["db-tickets-populate"]
    },
    {
      "name": "go google",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "args": ["go", "https://www.google.com/"]
    }
  ]
}
